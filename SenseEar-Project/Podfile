#platform :ios, '9.0'

target 'SenseEar-Project' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SenseEar-Project
  pod 'Firebase/Analytics'
  pod 'Firebase/Core'
